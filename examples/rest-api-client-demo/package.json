    "url": "https://github.com/kintone/js-sdk/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kintone/profile-loader": "^2.0.0",
    "@kintone/rest-api-client": "^2.0.8",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@kintone/customize-uploader": "^4.0.8",
    "@types/yargs": "^17.0.2",
    "ts-loader": "^8.3.0",
    "webpack": "^5.41.1",
    "webpack-cli": "4.7.2"
  }
