  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@kintone/profile-loader": "^1.1.1",
    "@kintone/rest-api-client": "^1.7.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@kintone/customize-uploader": "^3.0.10",
    "@types/yargs": "^15.0.9",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11"
