    "node": ">=10.0.0"
  },
  "dependencies": {
    "@kintone/profile-loader": "^1.1.2",
    "@kintone/rest-api-client": "^1.10.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@kintone/customize-uploader": "^3.1.5",
    "@types/yargs": "^16.0.0",
    "ts-loader": "^8.0.17",
    "webpack": "^5.23.0",
    "webpack-cli": "4.5.0"
  }
