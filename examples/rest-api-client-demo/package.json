    "lint:md": "prettier --check README.md",
    "lint:ts": "tsc --noEmit",
    "lint": "run-p -l lint:*",
    "deploy": "rimraf scripts/dist && run-s webpack upload",
    "upload": "kintone-customize-uploader customize-manifest.json",
    "run-script": "ts-node -r tsconfig-paths/register src/run-node.ts",
    "webpack": "webpack"
