{
  "name": "@kintone/js-sdk",
  "version": "1.0.0",
  "description": "packages for kintone customizations",
  "private": true,
  "scripts": {
    "prebuild": "pnpm -r --parallel run prebuild",
    "build": "pnpm prebuild && pnpm build:prod && pnpm postbuild",
    "build:prod": "tsc --build packages --force",
    "postbuild": "pnpm -r --parallel run postbuild",
    "lint": "pnpm -r --parallel run lint",
    "fix": "pnpm -r --parallel run fix",
    "initial:release": "pnpm prerelease && lerna publish from-package --conventional-commits",
    "prerelease": "npm-run-all -s build -p lint test",
    "release": "lerna publish --conventional-commits --create-release github",
    "pretest": "jest",
    "start": "pnpm build:prod --watch",
    "test": "pnpm -r --parallel run test",
    "test:ci": "pnpm pretest && pnpm -r run test:ci --stream"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kintone/js-sdk.git"
  },
  "keywords": [
    "kintone"
  ],
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@cybozu/eslint-config": "^18.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^13.13.52",
    "babel-jest": "^29.5.0",
    "comment-json": "^4.2.3",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "lerna": "^6.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {},
  "packageManager": "pnpm@8.6.3"
}
