{
  "name": "@kintone/customize-uploader",
  "version": "1.0.0",
  "description": "A kintone customize uploader",
  "bin": {
    "kintone-customize-uploader": "bin/cli.js"
  },
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "tsc -w --outDir ./dist/src",
    "build": "tsc --outDir ./dist/src",
    "lint": "run-p tsc tslint",
    "tsc": "tsc --noEmit",
    "tslint": "tslint -c tslint.json src/*.ts",
    "standard-version": "standard-version",
    "release": "run-s lint build standard-version"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/customize-uploader.git"
  },
  "keywords": [
    "kintone"
  ],
  "author": "kintone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/customize-uploader/issues"
  },
  "homepage": "https://github.com/kintone/customize-uploader#readme",
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/inquirer": "0.0.43",
    "@types/node": "^10.9.4",
    "@types/request-promise": "^4.1.42",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "inquirer": "^6.2.0",
    "meow": "^5.0.0",
    "os-locale": "^3.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
