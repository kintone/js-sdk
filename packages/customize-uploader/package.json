{
  "name": "@kintone/customize-uploader",
  "version": "3.1.0",
  "description": "A kintone customize uploader",
  "bin": {
    "kintone-customize-uploader": "bin/cli.js"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "lint": "run-p lint:*",
    "prerelease": "npm-run-all -p lint test -s build",
    "test": "jest --rootDir src",
    "test:ci": "jest --rootDir src --runInBand",
    "clean": "rimraf dist",
    "start": "tsc -w",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .ts src"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/customize-uploader"
  },
  "keywords": [
    "kintone"
  ],
  "author": "kintone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/js-sdk/issues"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/customize-uploader#readme",
  "devDependencies": {
    "@types/inquirer": "7.3.1",
    "@types/mkdirp": "^1.0.1",
    "@types/rimraf": "^3.0.0",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^1.7.2",
    "chokidar": "^3.4.3",
    "inquirer": "^7.3.3",
    "meow": "^9.0.0",
    "mkdirp": "^1.0.4",
    "os-locale": "^5.0.0",
    "rimraf": "^3.0.2"
  }
}
