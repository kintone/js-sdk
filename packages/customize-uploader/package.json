{
  "name": "@kintone/customize-uploader",
  "version": "3.0.1",
  "description": "A kintone customize uploader",
  "bin": {
    "kintone-customize-uploader": "bin/cli.js"
  },
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .ts src test",
    "release": "run-s lint build && standard-version",
    "release:dryrun": "standard-version --dry-run",
    "test": "mocha --require ts-node/register --watch-extensions ts test/**/*-test.ts"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/customize-uploader.git"
  },
  "keywords": [
    "kintone"
  ],
  "author": "kintone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/customize-uploader/issues"
  },
  "homepage": "https://github.com/kintone/customize-uploader#readme",
  "devDependencies": {
    "@cybozu/eslint-config": "^10.0.1",
    "@types/chokidar": "^1.7.5",
    "@types/inquirer": "6.5.0",
    "@types/mkdirp": "^1.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.17.17",
    "@types/request-promise": "^4.1.45",
    "@types/rimraf": "^2.0.3",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chokidar": "^3.3.1",
    "inquirer": "^7.0.6",
    "meow": "^6.0.1",
    "mkdirp": "^1.0.3",
    "os-locale": "^4.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2"
  }
}
