{
  "name": "@kintone/plugin-manifest-validator",
  "version": "3.0.5",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "files": [
    "index.js",
    "manifest-schema.d.ts",
    "manifest-schema.json",
    "src"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "yarn clean && tsc",
    "gen-dts": "node script/generate-dts.js",
    "prerelease": "run-p lint test",
    "test": "run-p test:js test:dts",
    "test:js": "jest",
    "test:dts": "tsc -p tsconfig.json",
    "test:ci": "yarn test",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .ts src",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@types/bytes": "^3.1.0",
    "ajv": "^6.12.3",
    "bytes": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "intelli-espower-loader": "^1.0.1",
    "json-schema-to-typescript": "^9.1.1",
    "power-assert": "^1.6.1"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/plugin-manifest-validator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/plugin-manifest-validator"
  },
  "bugs": {
    "url": "https://github.com/kintone/js-sdk/issues"
