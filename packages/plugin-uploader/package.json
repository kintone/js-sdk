  "bugs": {
    "url": "https://github.com/kintone/js-sdk/issues"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/main/packages/plugin-uploader#readme",
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "chalk": "^4.1.2",
