    "directory": "packages/plugin-uploader"
  },
  "bugs": {
    "url": "https://github.com/kintone/js-sdk/issues"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/plugin-uploader#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "meow": "^9.0.0",
    "os-locale": "^5.0.0",
    "puppeteer": "^21.0.2"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.6",
