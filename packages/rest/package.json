{
  "name": "@kintone/rest",
  "version": "0.0.1",
  "description": "kintone REST API Client for JavaScript",
  "keywords": [
    "kintone",
    "rest",
    "api-client"
  ],
  "homepage": "https://github.com/kintone/js-sdk/tree/main/packages/rest#readme",
  "bugs": {
    "url": "https://github.com/kintone/js-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/rest"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/es/index.js",
        "require": "./lib/cjs/index.js",
        "default": "./lib/es/index.js"
      },
      "browser": "./lib/es/index.browser.js"
    },
    "./package.json": "./package.json"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "types": "lib/es/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "esm",
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "pnpm run \"/^build:.*/\"",
    "build:lib": "vite build",
    "build:umd": "vite build --config vite.config.umd.ts",
    "clean": "rimraf lib esm umd",
    "fix": "pnpm lint:eslint --fix",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:package": "publint --strict",
    "start": "pnpm run watch",
    "test": "vitest",
    "test:ci": "vitest",
    "test:e2e": "vitest --config=vitest.e2e.config.ts",
    "watch": "pnpm run \"/^watch:.*/\"",
    "watch:lib": "pnpm run build:lib --mode development --watch",
    "watch:umd": "pnpm run build:umd --mode development --watch"
  },
  "dependencies": {
    "core-js": "^3.41.0",
    "fs": "0.0.1-security",
    "js-base64": "^3.7.7",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript": "^7.6.1",
    "openapi-typescript-helpers": "^0.0.15",
    "undici": "^7.4.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@cybozu/license-manager": "^1.2.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/rollup-plugin-node-builtins": "^2.1.5",
    "@types/rollup-plugin-node-globals": "^1.4.5",
    "@vitejs/plugin-legacy": "^6.0.2",
    "rollup": "^4.39.0",
    "rollup-plugin-ecma-version-validator": "^0.2.13",
    "rollup-plugin-license": "^3.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tsup": "^8.4.0",
    "vite": "^5.4.15",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-license": "^0.0.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
