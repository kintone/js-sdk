{
  "name": "@kintone/rest",
  "version": "0.0.1",
  "description": "kintone REST API Client for JavaScript",
  "main": "lib/src/index.js",
  "module": "esm/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "pnpm build:lib && pnpm build:esm",
    "build:lib": "tsc --build ./tsconfig.build.lib.json && cp -R src/schemas lib/src/schemas",
    "build:esm": "tsc --build ./tsconfig.build.esm.json && cp -R src/schemas esm/src/schemas",
    "clean": "rimraf lib esm umd",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "esm",
    "lib"
  ],
  "keywords": [
    "kintone"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "openapi-fetch": "^0.13.4",
    "openapi-typescript": "^7.6.0",
    "openapi-typescript-helpers": "^0.0.15"
  }
}
