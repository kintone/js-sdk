{
  "name": "kintone-typlify",
  "version": "0.0.1",
  "description": "Types for kintone js api and Types generating tools",
  "main": "dist/index.js",
  "scripts": {
    "build": "bin/npm-build.sh",
    "tsc": "tsc",
    "clean": "rm -rfv dist",
    "lint": "eslint --fix **/*.ts",
    "demo": "node dist/index.js --demo --type-name DemoFields -o demo-fields.d.ts",
    "generate": "node dist/index.js --host https://****.cybozu.com --username *** --password *** --app-id ***",
    "help": "node dist/index.js --help",
    "test": "jest",
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "build:integration": "webpack --mode development --config webpack.config.js"
  },
  "author": "Tomoya Yokota",
  "license": "MIT",
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "@types/nunjucks": "^3.1.0",
    "axios": "^0.18.0",
    "commander": "^2.19.0",
    "form-data": "^2.3.3",
    "lodash": "^4.17.11",
    "nunjucks": "^3.1.7",
    "prettier": "^1.15.3",
    "typescript-eslint-parser": "^21.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "eslint": "^5.12.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "bin": {
    "kintone-typlify": "dist/index.js"
  },
  "files": [
    "dist",
    "kintone.d.ts"
  ]
}
