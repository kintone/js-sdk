{
  "name": "<%= normalizePackageName(name.en) %>",
  "version": "0.1.0",
  "scripts": {
    <% if (enablePluginUploader) { %>
    "start": "node scripts/npm-start.js",
    "upload": "kintone-plugin-uploader dist/plugin.zip --watch --waiting-dialog-ms 3000",
    <% } else { %>
    "start": "npm run develop",
    <% } %>
    "develop": "npm run build -- --watch",
    "build": "kintone-plugin-packer --ppk private.ppk --out dist/plugin.zip src",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^14.0.3",
    "@kintone/plugin-packer": "^5.0.16",
    <% if (enablePluginUploader) { %>
    "@kintone/plugin-uploader": "^6.0.1",
    "npm-run-all": "^4.1.5",
    <% } %>
    "eslint": "^7.32.0"
  }
}
