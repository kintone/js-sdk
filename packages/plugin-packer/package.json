{
  "name": "@kintone/plugin-packer",
  "description": "Package your kintone plugin with pure JavaScript",
  "version": "6.0.19",
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.js",
  "bin": {
    "kintone-plugin-packer": "bin/cli.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "from-manifest.js"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc --build --force",
    "postbuild": "run-p js css",
    "clean": "rimraf dist",
    "lint": "eslint \"*.{js,ts}\" bin src site test",
    "test": "run-p jest site:test",
    "test:ci": "yarn test",
    "build:dev": "tsc --build --force --watch",
    "css": "postcss --config postcss.config.js ../../node_modules/normalize.css/normalize.css > docs/dist/normalize.min.css",
    "js": "webpack --mode production",
    "js:dev": "webpack serve --mode development",
    "jest": "jest",
    "site": "run-p js css",
    "site:dev": "run-p css js:dev",
    "site:test": "jest --config site/jest.config.js",
    "start": "npm-run-all -l -s clean build -p build:dev site:dev"
  },
  "dependencies": {
    "@kintone/plugin-manifest-validator": "^7.0.6",
    "chokidar": "^3.5.3",
    "debug": "^4.3.4",
    "denodeify": "^1.2.1",
    "meow": "^9.0.0",
    "mkdirp": "^1.0.4",
    "node-rsa": "~1.1.1",
    "stream-buffers": "^3.0.2",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "@types/debug": "^4.1.7",
    "@types/normalize-path": "^3.0.0",
    "@types/stream-buffers": "^3.0.4",
    "@types/yauzl": "^2.10.0",
    "@types/yazl": "^2.4.2",
    "ajv": "^6.12.6",
    "array-flatten": "^3.0.0",
    "assert": "^2.0.0",
    "babel-plugin-replace-ts-export-assignment": "^0.0.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "cssnano": "^5.1.14",
    "execa": "^5.1.1",
    "glob": "^8.0.3",
    "jest-environment-jsdom": "^29.1.2",
    "normalize-path": "^3.0.0",
    "normalize.css": "^8.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.0.0",
    "process": "^0.11.10",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "rimraf": "^3.0.2",
    "setimmediate": "^1.0.5",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "webpack": "^5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/plugin-packer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/plugin-packer"
  },
  "bugs": "https://github.com/kintone/js-sdk/issues",
  "keywords": [
    "kintone"
  ],
  "license": "MIT"
}
