{
  "name": "@kintone/plugin-packer",
  "description": "Package your kintone plugin with pure JavaScript",
  "version": "3.0.8",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "main": "src/index.js",
  "bin": {
    "kintone-plugin-packer": "bin/cli.js"
  },
  "files": [
    "bin",
    "src",
    "from-manifest.js"
  ],
  "scripts": {
    "build": "run-p js css",
    "lint": "npm run eslint",
    "test": "run-p jest site:test",
    "test:ci": "npm run test",
    "css": "postcss --config postcss.config.js ../../node_modules/normalize.css/normalize.css > docs/dist/normalize.min.css",
    "eslint": "eslint \"*.js\" bin src site test",
    "js": "webpack --mode production",
    "js:dev": "webpack serve --mode development",
    "jest": "jest",
    "prerelease": "run-p lint test build",
    "site": "run-p js css",
    "site:dev": "run-p css js:dev",
    "site:test": "jest --config site/jest.config.js"
  },
  "dependencies": {
    "@kintone/plugin-manifest-validator": "^3.0.9",
    "chokidar": "^3.4.3",
    "debug": "^4.2.0",
    "denodeify": "^1.2.1",
    "meow": "^8.0.0",
    "mkdirp": "^1.0.4",
    "node-rsa": "~1.1.1",
    "stream-buffers": "^3.0.2",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "array-flatten": "^3.0.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.7.1",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "cssnano": "^4.1.10",
    "execa": "^4.1.0",
    "glob": "^7.1.6",
    "normalize.css": "^8.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.2.0",
    "process": "^0.11.10",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "setimmediate": "^1.0.5",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.3",
    "webpack": "^5.4.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/plugin-packer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/plugin-packer"
  },
  "bugs": "https://github.com/kintone/js-sdk/issues",
  "keywords": [
    "kintone"
  ],
  "license": "MIT"
}
