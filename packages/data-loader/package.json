{
  "name": "@kintone/data-loader",
  "version": "0.12.1",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Cybozu, Inc.",
    "url": "https://cybozu.co.jp"
  },
  "description": "A data loader",
  "bin": {
    "kintone-data-loader": "cli.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc --build --force",
    "postbuild": "yarn pkg",
    "lint": "run-p -l lint:*",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "clean": "rimraf lib bin",
    "pkg": "pkg . -o bin/kintone-data-loader -t linux,macos,win",
    "fix:lint": "yarn lint:eslint -- --fix",
    "fix:md": "prettier --write README.md",
    "fix": "run-p fix:*",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:md": "prettier --check README.md",
    "start": "yarn build --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintone/js-sdk.git",
    "directory": "packages/data-loader"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "keywords": [
    "kintone"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kintone/js-sdk/issues"
  },
  "homepage": "https://github.com/kintone/js-sdk/tree/master/packages/data-loader#readme",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.11",
    "cross-env": "^7.0.3",
    "pkg": "^5.8.0"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^3.1.6",
    "csv-parse": "^4.16.3",
    "iconv-lite": "^0.6.3",
    "yargs": "^17.5.1"
  }
}
